    "def powerSet(string, index, curr):\n",
    "    if index == len(string):\n",
    "        print(curr)\n",
    "        return\n",
    "    \n",
    "    powerSet(string, index + 1,\n",
    "             curr + string[index])\n",
    "    powerSet(string, index + 1, curr)\n",
    "    \n",
    "# Driver code\n",
    "s1 = \"abc\"\n",
    "index = 0\n",
    "curr = \"\"\n",
    "print(powerSet(s1, index, curr))"
   ]
